{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id" : "externalWebResource.json",
    "title": "Definitions: External Web Resources.",
    "description": "Schemas in #/definitions detect or validate keys/objects used in describing External Web Resources (Section 3.2.1).",
    "definitions": {
        
        "externalWebResourceDetected": 
        {
            "type": "object",
            "allOf": [ {"$ref": "id.json#/definitions/idValueFound"} ],
            "not": 
            { "anyOf": 
              [ 
                 { "required": ["value"] },
                 { "required": ["source"] }
              ]
            }
        }
    }
}
