{
  "@context": "https://www.w3.org/ns/JSONtest-v1.jsonld",
  "name": "Annotation has all the required properties",
  "description": "Each annotation MUST have correctly formed @context, id, type, and target properties; references schemas in /definitions/*.json and /annotations/3.1-annotationMustKeys.json",
  "testType": "manual",
  "ref": "https://www.w3.org/TR/annotation-model/#model",
  "assertions": [
    "annotations/3.1-annotationMustKeys.json#/definitions/annotationContextValidated",
    "annotations/3.1-annotationMustKeys.json#/definitions/annotationIdValidated",
    "annotations/3.1-annotationMustKeys.json#/definitions/annotationTypeValidated",
    "annotations/3.1-annotationMustKeys.json#/definitions/notBodyBodyValue",
    "annotations/3.1-annotationMustKeys.json#/definitions/targetKeyFound",
    "annotations/3.1-annotationMustKeys.json#/definitions/targetObjectsRecognized" 
  ]
}
