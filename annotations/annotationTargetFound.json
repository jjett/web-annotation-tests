{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id" : "annotationTargetFound.json",
  "title": "Validation: Check if annotation has recognizable target(s)",
  "description": "True when any and all targets are recognizable as either a string/uri, a Choice/Set, a SpecificResource, or an External Web Resource (Sections 3.1, 3.2.7, 3.2.8 4, 3.2.1)",
  "type": "object",
      "properties": 
      {
        "target":
           {"oneOf": [
              { "type" : ["string", "object"], 
                "anyOf": [
                   {"$ref": "stringUri.json#"}, 
                   {"$ref": "choiceDetected.json#" }, 
                   {"$ref": "compositeDetected.json#" }, 
                   {"$ref": "independentsDetected.json#" }, 
                   {"$ref": "listDetected.json#" }, 
                   {"$ref": "specificResourceDetected.json#" },
                   {"$ref": "externalWebResourceDetected.json#" }
                 ]              
              },
              { "type" : ["array"], 
                "items": {"anyOf": [
                   {"$ref": "stringUri.json#"}, 
                   {"$ref": "choiceDetected.json#" }, 
                   {"$ref": "compositeDetected.json#" }, 
                   {"$ref": "independentsDetected.json#" }, 
                   {"$ref": "listDetected.json#" }, 
                   {"$ref": "specificResourceDetected.json#" },
                   {"$ref": "externalWebResourceDetected.json#" }
                ] } }
           ]      
      }
    }
}
