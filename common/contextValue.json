{
    "$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Check @context value",
    "description": "'http://www.w3.org/ns/anno.jsonld must be' an @contxt value (Section 3.1)",
    "assertionType": "must",
    "expectedResult": "invalid",
    "errorMessage": "Error: 'http://www.w3.org/ns/anno.jsonld' is not an @context value.",
    "type": "object",
    "properties": {"@context": {
        "oneOf": [
            { "type": "string",
              "not": {"$ref": "#/definitions/contextValue"} },     
            { "type": "array",
              "items": {"not": {"$ref": "#/definitions/contextValue"}}},
            { "type": "object"}
]
    }},
    "definitions": {"contextValue": {
        "enum": ["http://www.w3.org/ns/anno.jsonld"]
    }}
}
